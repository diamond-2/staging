{%- assign current_variant = product.selected_or_first_available_variant -%}
{% assign productMetafield = product.metafields.global %}
{% assign variantMetafield = current_variant.metafields.current_variant %}

{% assign hide_default_title = false %}
{% if product.options.size <= 1 or product.options.first.title contains 'Default' %}
  {% assign hide_default_title = true %}
{% endif %}

{%- comment -%}
{%- if product.variant.size == 1 and product.has_only_default_variant -%}
{%- assign delivery_days_instock = product.metafields.global.delivery_days_instock -%}
{%- else -%}
{%- assign delivery_days_instock = product.metafields.global.delivery_days_instock -%}
{%- endif -%}
{%- endcomment -%}
{%- capture delivery_days_instock -%}delivery_days_instock{%- endcapture -%}
{%- capture return_policy -%}return_policy{%- endcapture -%}
{%- capture shipping -%}shipping{%- endcapture -%}
{%- capture prod_info_dimension1  -%}prod_info_dimension1 {%- endcapture -%}
{%- capture prod_info_dimension2  -%}prod_info_dimension2{%- endcapture -%}
{%- capture prod_info_rhodium_plated -%}prod_info_rhodium_plated{%- endcapture -%}
{%- capture product_category -%}product_category{%- endcapture -%}
{%- capture jewelery_category -%}jewelery_category{%- endcapture -%}
{%- capture stone_category -%}stone_category{%- endcapture -%}
{%- capture prod_info_stone_coating -%}prod_info_stone_coating{%- endcapture -%}
{%- capture stone_color_granding -%}stone_color_granding{%- endcapture -%}
{%- capture shipping -%}shipping{%- endcapture -%}
{%- capture return_policy -%}return_policy{%- endcapture -%}
{%- capture warranty -%}warranty{% endcapture %}

<div class="product-template__container" id="ProductSection-{{ section.id }}" data-section-id="{{ section.id }}"
    data-section-type="product" data-enable-history-state="true" data-ajax-enabled="{{ settings.enable_ajax }}">
    <div class="dia-container">
        {% comment %}
        Get first variant, or deep linked one
        {% endcomment %}

        
        {%- assign product_image_zoom_size = '1024x1024' -%}
        {%- assign product_image_scale = '2' -%}
        {%- assign enable_image_zoom = section.settings.enable_image_zoom -%}
        {%- assign compare_at_price = current_variant.compare_at_price -%}
        {%- assign price = current_variant.price -%}

        <div class="product-section-row">
            <div class="product-detail-slider-grp" data-product-single-media-group>
                {% if product.media.size > 0 %}
                <div class="product-detail-slider-wrapper">
                    <div class="product-detail-slider-holder">
                        <div class="product-detail-slider">
                            {% for mediaSlide in product.media %}
                             
                            {%- if mediaSlide contains "_PDP" -%}
                            <div class="product-detail-slide {% if section.settings.enable_image_zoom == true %}image_zoom{% endif %}">
                                
                                {% if mediaSlide.media_type == 'image' %}
                                <div class="product-detail-slide-inner" data-scale="1.4">
                                    <div class="product-detail-slide-img-holder" style="background-image: url({{ mediaSlide | img_url:'900x' }});"></div>
                                </div>
                                {% elsif mediaSlide.media_type == 'video' %}
                                <div class="product-detail-slide-inner media_type_video" >
                                    <div class="product-detail-slide-img-holder">
                                        <video loop muted>
                                            {% for video in mediaSlide.sources %}
                                            <source src="{{ video.url }}" type="{{ video.mime_type }}">
                                            {% endfor %}
                                        </video>
                                    </div>
                                </div>
                                {% endif %}
                                
                            </div>
                            {%- endif -%}
                            {% endfor %}
                        </div>
                        <div class="hide_mobile">{% include 'product-badge' %}</div>
                        {% for mediaThumb in product.media %}
                        {% assign videoIndexValue = forloop.index0 %}
                        {% if mediaThumb.media_type == 'video' %}
                        <div class="show_mobile">
                            <div class="mobile-3d-img" data-index="{{ videoIndexValue }}">
                                {% include 'icon-view-360-mobile' %}
                            </div>
                        </div>
                        {% endif %}
                        {% endfor %}
                    </div>
                    <div class="product-detail-slider-thumbnail-wrapper hide_mobile">
                        <div class="right-border-line"></div>
                        <div class="product-detail-slider-thumbnail" {% if product.media.size <  5 %} data-size="{{ product.media.size }}"{% endif %}>
                            {% for mediaThumb in product.media %}
                            {%- if mediaThumb contains "_PDP" -%}
                            <div class="product-detail-slider-thumbnail-slide" data-alt="{{ mediaThumb.alt }}">
                                {% if mediaThumb.media_type == 'image' %}  
                                <div class="product-detail-slider-thumbnail-slide-inner">
                                    <div class="img_holder">
                                        <img data-src="{{ mediaThumb | img_url:'150x150' }}" class="lazyload"
                                        alt="{{ mediaThumb.alt }}">
                                    </div>
                                </div>
                                {% elsif mediaThumb.media_type == 'video' %}
                                <div class="product-detail-slider-thumbnail-slide-inner media_thumb">
                                    <div class="img_holder">{{ 'thumb-6.jpg' | asset_url | img_tag :'360 Video' }}</div>
                                </div>
                                {% endif %}
                            </div>
                            {%- endif -%}
                            {% endfor %}
                        </div>
                    </div>
                </div>
                {% endif %}

                {% if section.settings.show_share_buttons %}
                <div class="product-social-wrapper">
                    <div class="product-social-toggle">
                        {{ 'share-toggle.png' | asset_url | img_tag :'Share Icon' }}
                    </div>
                    <div class="product-social-box">
                        {% include 'social-sharing', share_title: product.title, share_permalink: product.url, share_image:
                        product.featured_media %}
                    </div>
                </div>
                {% endif %}
            </div>

            <div class="product-detail-content-column">
                <div class="product-detail-breadcrumb">
                    <ul>
                        <p style="display: none;">{{ template.suffix }}</p>
                        <li><a href="{{ routes.root_url }}">Home</a></li>
                        {%- if productMetafield[product_category] != blank -%}
                        <li><a href="{{ routes.collections_url }}/{{ productMetafield[product_category] | append: '-collections' | downcase | handleize }}">{{ productMetafield[product_category] }}</a></li>
                        {%- endif -%}
                        {% if template.suffix contains 'loose-stone' %}
                        {% if productMetafield[stone_category] != blank %}
                        <li><span class="breadcrumb_text">{{ productMetafield[stone_category] }}</span></li>
                        {% endif %}
                        {% else %}
                        {% if productMetafield[jewelery_category] != blank %}
                        <li><span class="breadcrumb_text">{{ productMetafield[jewelery_category] }}</span></li>
                        {% endif %}
                        {% endif %}
                    </ul>
                </div>
                <div class="product-single__meta">
                    {%- assign current_variant = product.selected_or_first_available_variant -%}
                    <h1 class="product-single__title">{{ product.title }}</h1>
                    {%- if template.suffix contains 'loose-stone' -%}
                    {%- else -%}
                    <div class="product-sku-wrapper">
                        {%- if current_variant.sku != blank -%}
                        <div class="p_sku">SKU: {{ current_variant.sku }}</div>
                        <div class="p_weight_guide"><a href="javascript:void(0);">Carat Weight Guide</a></div>
                        {%- endif -%}
                    </div>
                    {%- endif -%}
                    <div class="product-wishlist">
                        <div class="wishlist-text">SAVE TO WISHLIST</div>
                        <div class="wishlist-icon">
                            {% include 'iocn-wislist' %}
                        </div>
                    </div>
                   
                    {%- if cart.taxes_included or shop.shipping_policy.body != blank -%}
                    <div class="product__policies rte" data-product-policies>
                        {%- if cart.taxes_included -%}
                        {{ 'products.product.include_taxes' | t }}
                        {%- endif -%}
                        {%- if shop.shipping_policy.body != blank -%}
                        {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                        {%- endif -%}
                    </div>
                    {%- endif -%}

                    {% capture "form_classes" -%}
                    product-form product-form-{{ section.id }}
                    {%- if section.settings.enable_payment_button and product.has_only_default_variant %}
                    product-form--payment-button-no-variants {%- endif -%}
                    {%- if current_variant.available == false %} product-form--variant-sold-out {%- endif -%}
                    {%- endcapture %}

                    {% form 'product', product, class:form_classes, novalidate: 'novalidate', data-product-form: '' %}
                    {{ form | payment_terms }}

                    {% if template.suffix contains 'loose-stone' and  product.has_only_default_variant %}
                    {% if productMetafield[prod_info_stone_coating] != blank or productMetafield[stone_color_granding] != blank  %}
                    <ul class="stone-granding-options">
                        {% if productMetafield[prod_info_stone_coating] != blank %}
                        <li class="stone_coating">
                            <span class="stone_coating_label">Color - </span>
                            <span class="stone_coating_val">{{ productMetafield[prod_info_stone_coating] }}</span>
                        </li>
                        {% endif %}
                        {% if productMetafield[stone_color_granding] != blank %}
                        <li class="stone_color_granding">
                            <span class="stone_color_granding_label">Clarity - </span>
                            <span class="stone_color_granding_val">{{ productMetafield[stone_color_granding] }}</span>
                        </li>
                        {% endif %}
                    </ul>
                    {% endif %}
                    {% endif %}

                    {% unless product.has_only_default_variant %}
                    <div class="product-form__controls-group">
                        {% for option in product.options_with_values %}
                        <div class="selector-wrapper js product-form__item pdp-select-item">
                            {% assign indexVal = forloop.index %}

                            {% if option.name contains 'Color' or option.name contains 'Colour' or option.name contains 'COLOUR' %}
                            <label>SELECT GOLD COLOUR</label>
                            <div class="custom-checkbox-list">
                                {% for value in option.values %}
                                <div class="custom-checkbox">
                                    <input class="single-option-selector-{{ section.id }}" id="color-{{ forloop.index }}" type="radio" value="{{ value | escape }}"  data-index="option{{ indexVal }}" name="color" {% if option.selected_value==value %}checked{% endif %}>
                                    <span class="custom_check {{ value | downcase | replace:'gold','' | escape }}">{{ value | slice: 0, 3}}</span>
                                    <label for="color-{{ forloop.index }}">{{ value }}</label>
                                </div>
                                {% endfor %}
                            </div>

                            {% else %}
                            <div class="custom-select-holder">
                                <label for="SingleOptionSelector-{{ forloop.index0 }}">
                                    Select {% if product.metafields.global.jewelery_category != blank %}{{ product.metafields.global.jewelery_category }}{% endif %} {{ option.name }}
                                </label>
                                <select
                                    class="single-option-selector single-option-selector-{{ section.id }} product-form__input"
                                    id="SingleOptionSelector-{{ forloop.index0 }}" data-index="option{{ forloop.index }}">
                                    {% for value in option.values %}
                                    <option value="{{ value | escape }}" {% if option.selected_value==value %}
                                        selected="selected" {% endif %}>{{ value }}</option>
                                    {% endfor %}
                                </select>
                            </div>
                            {% if option.name =='Size' %}
                            <!-- Set Ring Size Text in Variable and Pass that value on 'ring-size-poup' -->
                            {% assign ringSizeText = 'Ring Size Guide' %}
                            <div class="ring-size-chart"><a custom-modal="trigger" href="#{{ ringSizeText | downcase | replace: ' ', '_' }}">{{ ringSizeText }}</a></div>
                            
                            {% capture ringSizePopup %}
                                {% include 'ring-size-popup', ring_size_text: ringSizeText %}
                            {% endcapture %}

                            {% endif %}
                            {% endif %}
                        </div>
                        {% endfor %}
                    </div>
                    {% endunless %}

                    <select name="id" id="ProductSelect-{{ section.id }}" class="product-form__variants no-js">
                        {% for variant in product.variants %}
                        <option data-title="{{ variant.title | handleize }}" data-color="{{ variant.option1 }}" data-size="{{ variant.option2 }}" value="{{ variant.id }}" {%- if variant==current_variant %} selected="selected" {%-endif -%}> {{ variant.title }} {%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %}
                        </option>
                        {% endfor %}
                    </select>

                    {% if section.settings.show_quantity_selector %}
                    <div class="product-form__controls-group">
                        <div class="product-form__item">
                            <label for="Quantity-{{ section.id }}">{{ 'products.product.quantity' | t }}</label>
                            <input type="number" id="Quantity-{{ section.id }}" name="quantity" value="1" min="1"
                                pattern="[0-9]*" class="product-form__input product-form__input--quantity"
                                data-quantity-input>
                        </div>
                    </div>
                    {% endif %}

                    <div class="product-form__error-message-wrapper product-form__error-message-wrapper--hidden{% if section.settings.enable_payment_button %} product-form__error-message-wrapper--has-payment-button{% endif %}"
                        data-error-message-wrapper role="alert">
                        <span class="visually-hidden">{{ 'general.accessibility.error' | t }} </span>
                        {% include 'icon-error' %}
                        <span class="product-form__error-message" data-error-message>{{
                            'products.product.quantity_minimum_message' | t }}</span>
                    </div>

                    <div class="product-form__controls-group product-form__controls-group--submit">
                        <div class="product-form__item product-form__item--submit
                    {%- if section.settings.enable_payment_button %} product-form__item--payment-button {%- endif -%}
                    {%- if product.has_only_default_variant %} product-form__item--no-variants {%- endif -%}">
                            <button type="submit" name="add" {% unless current_variant.available %} aria-disabled="true"
                                {% endunless %}
                                aria-label="{% unless current_variant.available %}{{ 'products.product.sold_out' | t }}{% else %}{{ 'products.product.add_to_cart' | t }}{% endunless %}"
                                class="btn product-form__cart-submit glb-btn{% if section.settings.enable_payment_button and product.selling_plan_groups == empty %} btn--secondary-accent{% endif %}"
                                {% if settings.enable_ajax %}aria-haspopup="dialog" {% endif %} data-add-to-cart>

                                <div class="product__price">
                                    {% include 'product-price', variant: current_variant, show_vendor: section.settings.show_vendor
                                    %}
                                </div>

                                <span data-add-to-cart-text>
                                    {% unless current_variant.available %}
                                    {{ 'products.product.sold_out' | t }}
                                    {% else %}
                                    ADD TO BAG
                                    {% endunless %}
                                </span>
                                <span class="hide" data-loader>
                                    {% include 'icon-spinner' %}
                                </span>
                            </button>
                            {% if section.settings.enable_payment_button %}
                            {{ form | payment_button }}
                            {% endif %}
                        </div>
                    </div>
                    {% endform %}

                </div>

                <div class="price_inclusive">Price is Inclusive of Taxes</div>
                <div class="delivery-check-sec">
                    <div class="delivery-check-title">check delivery date</div>
                    <div class="delivery-check-input">
                        <span class="delivery-icon"><svg width="10" height="16" viewBox="0 0 10 16" fill="none" xmlns="http://www.w3.org/2000/svg">
    <path d="M5 15.8925L4.69231 15.5228C4.5 15.3174 0 9.64919 0 6.24003C0 3.28269 2.23077 0.900391 5 0.900391C7.76923 0.900391 10 3.28269 10 6.24003C10 9.64919 5.5 15.3174 5.30769 15.5639L5 15.8925ZM5 1.72187C2.65385 1.72187 0.769231 3.73451 0.769231 6.24003C0.769231 8.86878 4 13.3048 5 14.6192C6 13.3048 9.23077 8.86878 9.23077 6.24003C9.23077 3.73451 7.34615 1.72187 5 1.72187Z" fill="black"/>
    <path d="M5.00024 8.39404C3.84729 8.39404 2.94141 7.43208 2.94141 6.20776C2.94141 4.98345 3.84729 4.02148 5.00024 4.02148C6.15319 4.02148 7.05908 4.98345 7.05908 6.20776C7.05908 7.43208 6.15319 8.39404 5.00024 8.39404ZM5.00024 4.896C4.30024 4.896 3.76494 5.46443 3.76494 6.20776C3.76494 6.9511 4.30024 7.51953 5.00024 7.51953C5.70025 7.51953 6.23555 6.9511 6.23555 6.20776C6.23555 5.46443 5.70025 4.896 5.00024 4.896Z" fill="black"/>
    </svg></span>
                        <input type="text" id="delivery-check" placeholder="Enter pincode">
                    </div>
                    {%- if productMetafield[delivery_days_instock] != blank -%}
                    <div class="delivery-check-date">{{ productMetafield[delivery_days_instock] }}</div>
                    {%- endif -%}
                </div>

                <div class="product-details-btn-grid">
                    <div class="product-details-btn-item full-width-btn">
                        <a class="pdp-white-btn" href="#">SHOP AT HOME WITH OUR AMBASSADOR</a>
                    </div>
                    <div class="product-details-btn-item">
                        <a class="pdp-white-btn" href="#">VIRTUAL TRY ON</a>
                    </div>
                    <div class="product-details-btn-item">
                        <a class="pdp-white-btn" href="#">TRY AT HOME</a>
                    </div>
                </div>

                <div class="pdp-connect-section">
                    <div class="connect-with-phone">
                        <div class="left_col">
                            <span>
                                <svg width="30" height="30" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M28.126 22.9376C27.0572 21.8609 24.4685 20.2895 23.2125 19.6563C21.5768 18.8326 21.4422 18.7653 20.1566 19.7202C19.2991 20.3575 18.729 20.9268 17.7254 20.7128C16.7218 20.4988 14.541 19.2922 12.6314 17.3891C10.7219 15.4859 9.44501 13.2423 9.2303 12.2423C9.01558 11.2423 9.59444 10.679 10.2258 9.81965C11.1156 8.60833 11.0483 8.40644 10.2877 6.77116C9.69473 5.49927 8.07729 2.93531 6.9963 1.87204C5.83993 0.73003 5.83993 0.931917 5.09481 1.24148C4.4882 1.49665 3.90624 1.80684 3.35621 2.16814C2.27926 2.88349 1.68156 3.47771 1.26356 4.37072C0.845574 5.26374 0.657781 7.3573 2.81639 11.2779C4.975 15.1986 6.48946 17.2033 9.62406 20.3285C12.7587 23.4537 15.169 25.1341 18.6926 27.1099C23.0516 29.5507 24.7236 29.075 25.6194 28.6577C26.5153 28.2405 27.1124 27.6483 27.8292 26.5716C28.1915 26.0226 28.5025 25.4414 28.7581 24.8353C29.0684 24.0931 29.2703 24.0931 28.126 22.9376Z" stroke="black" stroke-width="1.5" stroke-miterlimit="10"/>
                                    </svg>                                    
                            </span>
                            <span class="text">to order by  phone</span>
                        </div>
                        <div class="right_col">
                            <a href="tel:+8002278400">1-800-227-8400</a>
                        </div>
                    </div>
                    <div class="pdp-warranty-sec">
                        {% if variantMetafield[return_policy] != blank  or productMetafield[return_policy] != blank %}
                        <div class="pdp-warranty-item _returns">
                            <span>
                                <svg width="18" height="20" viewBox="0 0 18 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M9 4V0L3.375 5L9 10V6C12.7238 6 15.75 8.69 15.75 12C15.75 15.31 12.7238 18 9 18C5.27625 18 2.25 15.31 2.25 12H0C0 16.42 4.0275 20 9 20C13.9725 20 18 16.42 18 12C18 7.58 13.9725 4 9 4Z" fill="black"/>
                                    </svg>                                    
                            </span>
                            {% if variantMetafield[return_policy] != blank %}
                                <span class="text return_policy_val">{{ variantMetafield[return_policy] }}</span>
                            {% else %}
                                {% if productMetafield[return_policy] != blank %}
                                    <span class="text return_policy_val">{{ productMetafield[return_policy] }}</span>
                                {% endif %}
                            {% endif %}
                        </div>
                        {% endif %}
                        {% if variantMetafield[warranty] != blank  or productMetafield[warranty] != blank %}
                        <div class="pdp-warranty-item _warranty">
                            <span>
                            <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path fill-rule="evenodd" clip-rule="evenodd" d="M6.19705 1.988C4.8369 2.3158 3.48557 2.67235 2.14419 3.05733C2.0228 3.09093 1.91444 3.15454 1.83157 3.24086C1.7487 3.32719 1.69467 3.43272 1.6757 3.54533C0.882972 8.74133 2.71234 12.5333 4.89419 15.0307C5.98883 16.284 7.1742 17.212 8.16176 17.8213C8.65702 18.1253 9.0928 18.3467 9.43636 18.488C9.60889 18.5587 9.74869 18.6067 9.85578 18.6347C9.903 18.6483 9.9512 18.659 10 18.6667C10.0105 18.6653 10.0566 18.66 10.1443 18.636C10.2514 18.6067 10.3912 18.5587 10.5637 18.488C10.9058 18.3467 11.3446 18.1253 11.8383 17.8213C13.0767 17.0424 14.1781 16.1018 15.1059 15.0307C17.2878 12.5333 19.1171 8.74133 18.3244 3.54533C18.3054 3.43272 18.2514 3.32719 18.1685 3.24086C18.0856 3.15454 17.9773 3.09093 17.8559 3.05733C16.9249 2.79067 15.3543 2.356 13.803 1.988C12.2176 1.61333 10.7586 1.33333 10 1.33333C9.24153 1.33333 7.78399 1.61333 6.19705 1.988ZM5.8163 0.7C7.36755 0.332 9.01397 0 10 0C10.9846 0 12.6325 0.332 14.1838 0.7C15.5674 1.03303 16.942 1.39536 18.3065 1.78667C19.0918 2.01067 19.6838 2.61733 19.7983 3.36533C20.6505 8.96133 18.6739 13.108 16.2779 15.852C15.2568 17.0296 14.0446 18.0633 12.6816 18.9187C12.2077 19.2163 11.7067 19.4776 11.1839 19.7C10.7824 19.8653 10.354 20 10 20C9.64607 20 9.21773 19.8653 8.81616 19.7C8.29328 19.4778 7.79227 19.2165 7.31847 18.9187C5.95602 18.0632 4.7443 17.0295 3.72369 15.852C1.3247 13.108 -0.650422 8.96133 0.201794 3.36533C0.258773 3.00276 0.429121 2.66207 0.693121 2.38269C0.95712 2.1033 1.30398 1.89666 1.69354 1.78667C3.05805 1.39537 4.43269 1.03304 5.8163 0.7Z" fill="black"/>
