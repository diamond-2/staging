<script>
    $('.product-detail-slider').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: true,
        fade: true,
        asNavFor: '.product-detail-slider-thumbnail',
        draggable: false,
        infinite: false,
        autoplay:false,
        responsive: [
            {
                breakpoint: 749,
                settings: {
                    dots: true
                }
            }
        ]
    });
    $('.product-detail-slider-thumbnail').slick({
        slidesToShow: 5,
        slidesToScroll: 1,
        vertical:true,
        asNavFor: '.product-detail-slider',
        dots: false,
        verticalSwiping:true,
        focusOnSelect: true,
        draggable: false,
        arrows: false,
        infinite: false,
        autoplay:false,
        centerMode: false,
        responsive: [
            {
                breakpoint: 991,
                settings: {
                    verticalSwiping:false,
                    vertical:false
                }
            }
        ]
    }); 
    
    // Go to 360 view Slide Mobile
    $('.mobile-3d-img').click(function(e) {
        var slideno = $(this).attr('data-index');
        $('.product-detail-slider').slick('slickGoTo', slideno);
        $('.product-detail-slider .product-detail-slide[data-slick-index="'+slideno+'"]').find('video').get(0).play();
    });
    
    function activeBorder() {
        setTimeout(function(){
            $(".slide-active-border").css({
                height: $(".product-detail-slider-thumbnail .slick-slide.slick-current .product-detail-slider-thumbnail-slide-inner").height(),
                top: $(".product-detail-slider-thumbnail .slick-slide.slick-current").position().top
            });
        },100);
        $(".product-detail-slider-thumbnail .slick-slide").click(function() {
            var borederHeight = $(this).children('.product-detail-slider-thumbnail-slide-inner').height();
                borederPos = $(this).children('.product-detail-slider-thumbnail-slide-inner').position().top;
            $(".slide-active-border").css({
                height: borederHeight,
                top: borederPos
            })
        });
    }
    window.addEventListener("load", function(){
        $('.product-detail-slider-thumbnail .slick-track').prepend('<div class="slide-active-border"></div>');
        activeBorder();
        // border mover on Arrow click
        $(".product-detail-slider-wrapper .slick-arrow").click(function() {
            $(".slide-active-border").css({
                height: $(".product-detail-slider-thumbnail .slick-slide.slick-current .product-detail-slider-thumbnail-slide-inner").height(),
                top: $(".product-detail-slider-thumbnail .slick-slide.slick-current").position().top
            });
        });    
    });
    $(window).resize(function(){
        activeBorder();
    });


// slick zoom
$(".image_zoom .product-detail-slide-inner")
  .on("mouseover", function() {
    $(this)
      .children(".product-detail-slide-img-holder")
      .css({ transform: "scale(" + $(this).attr("data-scale") + ")" });
  })
  .on("mouseout", function() {
    $(this)
      .children(".product-detail-slide-img-holder")
      .css({ transform: "scale(1)" });
  })
  .on("mousemove", function(e) {
    $(this)
      .children(".product-detail-slide-img-holder")
      .css({
        "transform-origin":
          ((e.pageX - $(this).offset().left) / $(this).width()) * 100 +
          "% " +
          ((e.pageY - $(this).offset().top) / $(this).height()) * 100 +
          "%"
      });
  });
</script>

