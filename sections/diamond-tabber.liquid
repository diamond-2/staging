                                                    {% if item.settings.image_6 %}<img class="lazyload" data-src="{{ item.settings.image_6 | img_url: 'master' }}" alt="{{ item.settings.title }}">{% endif %}
                                                    <span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip6_title }}</span>
