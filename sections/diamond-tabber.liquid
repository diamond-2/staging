<div class="diamond-tabber-wrapper">
    <div class="dia-container">
        {% if section.settings.heading !=blank %}
        <div class="diamond-tabber-sec-header">
            <h2>{{ section.settings.heading }}</h2>
        </div>
        {% endif %}

        <div class="diamond-tabber">
            
            <div class="diamond-tabber-nav">
                <ul>
                    {% for item in section.blocks %}
                    {% if item.settings.title !=blank %}<li {% if forloop.first %}class="active"{% endif %}><a href="#diamond-tab{{ forloop.index }}">{{ item.settings.title }}</a></li>{% endif %}
                    {% endfor %}
                </ul>
            </div>
            <div class="diamond-tabber-panal-wrapper">
                {% for item in section.blocks %}
                <div class="diamond-tabber-panal {% if forloop.first %}show_tab{% endif %}" id="diamond-tab{{ forloop.index }}">
                    <div class="diamond-tabber-panal-inner">
                        {% if item.settings.title !=blank or item.settings.content !=blank %}
                        <div class="diamond-tabber-body-top">
                            {% if item.settings.title !=blank %}<h2>{{ item.settings.title }}</h2>{% endif %}
                            {% if item.settings.content !=blank %}{{ item.settings.content }}{% endif %}
                        </div>
                        {% endif %}

                        <div class="diamond-tabber-body-bottom">
                            {% if item.type == 'two-col-image-tabber' %}
                                <div class="tooltip_with_img_contain">
                                    <div class="two-col-tab-images">
                                        {% if item.settings.image_1 %}
                                        <div class="image_col_1">
                                            <img class="lazyload" data-src="{{ item.settings.image_1 | img_url: 'master' }}" alt="{{ item.settings.title }}">
                                        </div>
                                        {% endif %}
                                        {% if item.settings.image_2 %}
                                        <div class="image_col_1">
                                            <img class="lazyload" data-src="{{ item.settings.image_2 | img_url: 'master' }}" alt="{{ item.settings.title }}">
                                        </div>
                                        {% endif %}
                                    </div>
                                    <div class="diamond-tabber-tooltips-sec">
                                        <ul>
                                            {% if item.settings.tooltip1_title !=blank %}<li><a href="#" data-tooltip-content="{{ item.settings.tooltip1_content }}"><span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip1_title }}</span></a></li>{% endif %}
                                            {% if item.settings.tooltip2_title !=blank %}<li><a href="#" data-tooltip-content="{{ item.settings.tooltip2_content }}"><span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip2_title }}</span></a></li>{% endif %}
                                            {% if item.settings.tooltip3_title !=blank %}<li><a href="#" data-tooltip-content="{{ item.settings.tooltip3_content }}"><span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip3_title }}</span></a></li>{% endif %}
                                            {% if item.settings.tooltip4_title !=blank %}<li><a href="#" data-tooltip-content="{{ item.settings.tooltip4_content }}"><span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip4_title }}</span></a></li>{% endif %}
                                            {% if item.settings.tooltip5_title !=blank %}<li><a href="#" data-tooltip-content="{{ item.settings.tooltip5_content }}"><span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip5_title }}</span></a></li>{% endif %}
                                        </ul>
                                        <div class="diamond-tabber-tooltips-box"></div>
                                    </div>
                                </div>
                            {% endif %}

                            {% if item.type == 'six-col-image-tabber' %}
                                <div class="tooltip_with_img_contain more_column six_column_container">
                                    <div class="diamond-tabber-tooltips-sec">
                                        <ul>
                                            {% if item.settings.tooltip1_title !=blank %}
                                            <li>
                                                <a href="#" data-tooltip-content="{{ item.settings.tooltip1_content }}">
                                                    {% if item.settings.image_1 %}<img class="lazyload" data-src="{{ item.settings.image_1 | img_url: 'master' }}" alt="{{ item.settings.title }}">{% endif %}
                                                    <span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip1_title }}</span>
                                                </a>
                                            </li>
                                            {% endif %}
                                            {% if item.settings.tooltip2_title !=blank %}
                                            <li>
                                                <a href="#" data-tooltip-content="{{ item.settings.tooltip2_content }}">
                                                    {% if item.settings.image_2 %}<img class="lazyload" data-src="{{ item.settings.image_2 | img_url: 'master' }}" alt="{{ item.settings.title }}">{% endif %}
                                                    <span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip2_title }}</span>
                                                </a>
                                            </li>
                                            {% endif %}
                                            {% if item.settings.tooltip3_title !=blank %}
                                            <li>
                                                <a href="#" data-tooltip-content="{{ item.settings.tooltip3_content }}">
                                                    {% if item.settings.image_3 %}<img class="lazyload" data-src="{{ item.settings.image_3 | img_url: 'master' }}" alt="{{ item.settings.title }}">{% endif %}
                                                    <span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip3_title }}</span>
                                                </a>
                                            </li>
                                            {% endif %}
                                            {% if item.settings.tooltip4_title !=blank %}
                                            <li>
                                                <a href="#" data-tooltip-content="{{ item.settings.tooltip4_content }}">
                                                    {% if item.settings.image_4 %}<img class="lazyload" data-src="{{ item.settings.image_4 | img_url: 'master' }}" alt="{{ item.settings.title }}">{% endif %}
                                                    <span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip4_title }}</span>
                                                </a>
                                            </li>
                                            {% endif %}
                                            {% if item.settings.tooltip5_title !=blank %}
                                            <li>
                                                <a href="#" data-tooltip-content="{{ item.settings.tooltip5_content }}">
                                                    {% if item.settings.image_5 %}<img class="lazyload" data-src="{{ item.settings.image_5 | img_url: 'master' }}" alt="{{ item.settings.title }}">{% endif %}
                                                    <span class="tooltip_title"><span class="tol_line"></span>{{ item.settings.tooltip5_title }}</span>
