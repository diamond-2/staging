<script type="text/javascript">
  $(document).on('click', '#enroll-btn', function(){
    console.log('first');
    $('#enroll-registration-form').show();
  });
  $(document).on('click', '#close-popup', function(){
    console.log('second');
    $('#enroll-registration-form').hide();
  });

  var input = document.querySelector(".contactPhoneNumer");
    window.intlTelInput(input,({
    initialCountry: "SG"
  }));

  $(document).ready(function() {
    $('.iti__flag-container').click(function() { 
      $('.contactPhoneNumer').val('');
    var countryCode = $('.iti__selected-flag').attr('title');
    var countryCode = countryCode.replace(/[^0-9]/g,'')
    //$('.iti--allow-dropdown .contactPhoneNumer').val("");
    $('.contactPhoneNumer').val("+"+countryCode+" "+ $('.contactPhoneNumer').val());
    });
  });

  $(document).ready(function() {
    $('.contactPhoneNumer').keypress(function(e) {
      var verified = (e.which == 8 || e.which == undefined || e.which == 0) ? null : String.fromCharCode(e.which).match(/[^0-9]/);
      if (verified) {e.preventDefault();}
    });
  });

  /* Validation */
  $(document).on('click', '#form_submit', function(){
    if($('#first_name').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }
      if($('#last_name').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }
      if($('#email').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }else if(!isValidEmail($('#email').val()))
      {
          $('.error-2').css("display" , "block");
          return false;
      }
      if($('#company_name').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }
      if($('#address').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }
      if($('#city').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }
      if($('#postal_code').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }else if(!isValidZip($('#postal_code').val()))
      {
          $('.error-2').css("display" , "block");
          return false;
      }
      if($('#phone').val()=='')
      {
          $('.error-1').css("display" , "block");
          return false;
      }
  });


function isValidZip(zip) {
   var pattern = new RegExp(/^([0-9]{5})(-[0-9]{4})?$/i);
  return pattern.test(zip);
 };
 function isValidEmail(email) {
   var pattern = new RegExp(/^([0-9]{5})(-[0-9]{4})?$/i);
  return pattern.test(email);
 };
</script>

