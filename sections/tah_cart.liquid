
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
<link rel="stylesheet" href="{{ 'tah_cart.css' | asset_url }}" type="text/css" >
<div id="shopify-section-cart-template-t-cart" class="shopify-section">
    <div class="cart-template-wrapper padding_t">
        <div class="dia-container" data-section-id="cart-template" data-section-type="cart-template" data-ajax-enabled="true">
          <div data-cart-wrapper="">
            <div class="glb-breadcrumb">
              <ul>
                <li>
                  <a href="/">Home</a>
                  <span class="breadcrumb_divider">/</span>
                </li>
                <li><span class="bred_tex" id="breadcrumb-text-c" style="color:inherit;">Shopping Bag</span></li>
              </ul>
            </div>
            <div class="update-cart" id="tah-cart-form">
              <div class="cart-page-se-header">
                <div class="c_heading">{{ section.settings.section_heading-ct }}</div>
                <div class="total_cart_item">Items ( <span id="th_cart_items_no"><span id="th_cart_t_no"></span>/7</span> ) {{ section.settings.section_heading-csb }}</div>
              </div>
              <form action="/cart" method="post" novalidate="" class="cart">
                <div class="cart-page-two-col-row">
                  <div class="cart-page-left-column">
                    <table>
                      <tbody id="the_cart_items_render"> 
                        </tbody>
                    </table>
                  </div>
                  <div class="cart-page-right-column">
                    <div class="coupon-info">
                      <div class="coupon-info-icon">
                        <img src="{% if section.settings.cart-item-t34  != blank %} {{ section.settings.cart-item-t34 |img_url:"master"}} {% else %}https://cdn.shopify.com/s/files/1/0575/2681/2840/files/check-mark-svgrepo-com.svg?v=1648748830{% endif %}">                  
                      </div>
                      <div class="coupon-info-content-">
                        <p>{{ section.settings.cart-item-tl }}</p>
                      </div>
                      <div class="cart-right-greater-iocn">
                        <img src="{% if section.settings.cart-item-t35  != blank %} {{ section.settings.cart-item-t35 |img_url:"master"}} {% else %}https://cdn.shopify.com/s/files/1/0575/2681/2840/files/greater-than-solid-svgrepo-com_1.svg?v=1648810255{% endif %}">
                      </div>
                    </div>
                    <div class="cart_sum_title">{{ section.settings.cart-item-t2 }}</div>
                    <div class="cart__footer">
                      <div class="grid">
                        <div class="grid__item delivery-notes">
                          <span class="delivery-notes-heading">{{ section.settings.cart-item-t3 }}</span>
                          <span class="delivery-notes-subheading">INR O</span>
                        </div>
                        <div class="grid__item service-charge">
                          <span class="delivery-notes-heading">{{ section.settings.cart-item-t4 }}</span>
                          <span class="delivery-notes-subheading">{{ section.settings.cart-item-t5 }}</span>
                        </div>
                        <div class="grid__item cart_total_item ">
                          <div class="cart-subtotal">
                            <span class="cart-subtotal__title_">{{ section.settings.cart-item-t6 }}</span>
                            <span class="cart-subtotal__price" data-cart-subtotal="">INR 0</span>
                          </div>
                        </div>
                      </div>
                    </div>
      
                    <div class="cart_bottom_section">
                      <div class="cart__buttons-container-r" id="sap-btn">
                        <div class="cart__submit-controls-">
                          <a href="javascript:void(0);" class="glb-btn">{{ section.settings.cart-item-t7 }}</a>
                        </div>
                        <div class="cart__continue-controls">
                          <a href="/" class="glb-btn">{{ section.settings.cart-item-t8 }}</a>
                        </div>
                    <div class="extra-info-box">
                      <p>{{ section.settings.cart-item-t9 }}</p>
                    </div>
                  </div>
                </div>
              </form>
            </div>
    
           
          </div>
          <div class="empty-cart  hide" data-empty-page-content="">
              <h2>Your Shopping Bag</h2>
              <div class="content_holder">
                  <p>Your shopping bag is empty.</p>
                  <p>For faster checkout, sign in to your account.</p>
              </div>
              <a href="/account/register" class="glb-btn-with-arrow">SIGN IN or SIGN UP</a>       
          </div>
        </div>
      </div>
    </div>
    <div class="try-at-home-cart-describing" id="tah-cart-describing">
    <div class="describing-container">
      <p class="section-heading">{{ section.settings.section_heading-d }}</p>
      <p class="sub-heading">{{ section.settings.section_subheading-d }}</p>
     
      <div class="describe-boxes"> 
     {% for block in section.blocks %} 
     {% if block.type == 'addBlocks' %}
        <div class="inner-explained-section">
           <img class="small-img-1" src="{% if block.settings.small-img-d  !=blank %}{{ block.settings.small-img-d | img_url:"master" }}{% else %} https://cdn.shopify.com/s/files/1/0575/2681/2840/files/Try_at_Home__Shopping_b_1.jpg?v=1648807539{% endif %}">
           <p class="small-text-1">{{ block.settings.small-text-1-d }} </p>
           <p class="small-text-2">{{ block.settings.small-text-2-d }}</p>
        </div>
        {% endif %}
        {% endfor %}
      </div>
     </div>
    </div>
    
     <!-- checkout code -->
     <div class="checkout-template-wrapper dia-container" id="checkout-form">
      <div class="checkout-page-two-col-row">
    
        <div class="checkout-page-left-column">
         <form class="checkout-form">
           <div class="date-slot-wrapper">
           <div class="date-slot-label">
             <h2 class="date-slot-label-h">{{ section.settings.cart-item-t10 }}</h2>
           </div>
           <div class="date-slot-input">
             <!-- <input class="date-slot-input-date" type="date" data-date-inline-picker="true" value="" name="date"/> -->
