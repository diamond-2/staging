                                                </tr>
                                                {% endif %}
                                                
                                                {% if variantMetafield[tableContent_dimension2] != blank %}
                                                <tr data-tooltip="jewdim2" data-row="{{ tableContent_dimension2 }}">
                                                    <td><span class="pdp-tab-label">{% if  variantMetafield[tableContent_dimension2Label] != blank %}{{  variantMetafield[tableContent_dimension2Label]}} {% else %}Width{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_dimension2] }}</span> {%- if productMetafield[tableContent_tooltip_width] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_width] }}"></span>{%- endif -%}</td>
                                                </tr>
                                                {% endif %}

                                                {% if variantMetafield[tableContent_dimension3] != blank %}
                                                <tr data-tooltip="jewdim3" data-row="{{ tableContent_dimension3 }}">
                                                    <td><span class="pdp-tab-label">{% if  variantMetafield[tableContent_dimension3Label] != blank %}{{  variantMetafield[tableContent_dimension3Label]}} {% else %}Width{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_dimension3] }}</span> {%- if productMetafield[tableContent_tooltip_width] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_width] }}"></span>{%- endif -%}</td>
                                                </tr>
                                                {% endif %}

                                                
                                                {% if variantMetafield[tableContent_dimension4] != blank %}
                                                <tr data-tooltip="jewdim3" data-row="{{ tableContent_dimension4 }}">
                                                    <td><span class="pdp-tab-label">{% if  variantMetafield[tableContent_dimension4Label] != blank %}{{  variantMetafield[tableContent_dimension4Label]}} {% else %}Width{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_dimension4] }}</span> {%- if productMetafield[tableContent_tooltip_width] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_width] }}"></span>{%- endif -%}</td>
                                                </tr>
                                                {% endif %}
                                                
                                                {% if variantMetafield[tableContent_rhodium_plated] != blank %}
                                                <tr data-tooltip="jewrhodium" data-row="{{ tableContent_rhodium_plated }}">
                                                    <td><span class="pdp-tab-label">{% if variantMetafield[tableContent_rhodium_platedLabel] != blank %}{{ variantMetafield[tableContent_rhodium_platedLabel] }}{% else %}Rhodium plated{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_rhodium_plated] | truncate: 30, '...' }}</span> {% if productMetafield[tableContent_tooltip_rhodium] != blank %} <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_rhodium]}}"></span>{% endif %}</td>
                                                </tr>
                                                {% endif %}

                                                {% if variantMetafield[tableContent_frc_ttl_crt_wgt] != blank %}
                                                <tr data-tooltip="jewrhodium" data-row="common_ttl_crt_wgt">
                                                    <td><span class="pdp-tab-label">Carat Total weight(CT. TW.)</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_frc_ttl_crt_wgt] | truncate: 30, '...' }}</span> {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt]}}"></span>{% endif %}</td>
                                                </tr>
                                                {% endif %}

                                                {% if variantMetafield[tableContent_dec_ttl_crt_wgt] != blank %}                                                <tr data-tooltip="jewrhodium" data-row="common_ttl_crt_wgt">
                                                    <td><span class="pdp-tab-label">Total carat weight(cts.)</span></td>
                                                    <td>
                                                    <span class="pdp-tab-val test-2">{{ variantMetafield[tableContent_dec_ttl_crt_wgt] | times: 100 | round:2 | money_without_currency  }}</span> 

                                                    {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} 
                                                    <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt] }}"></span>
                                                    {% endif %}
                                                    
                                                    </td>
                                                </tr>
                                                {% endif %}
                                        </tbody>
                                    </table>
                                </figure>
                            </div>
                        </div>
                    </div>
                    {% endif %}
                    
                    {%- capture tableContent_rstab_title-%}rstab_size1_title{%- endcapture -%}
                    {%- capture tableContent_rstab1-%}rstab_size1_row1{%- endcapture -%}
