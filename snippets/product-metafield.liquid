                    {% if
                        productMetafield[prodtableContent_metalLabel] != blank or
                        productMetafield[prodtableContent_title] != blank or 
                        variantMetafield[tableContent_dimension1Label] != blank or
                        variantMetafield[tableContent_dimension1] != blank or 
                        variantMetafield[tableContent_dimension2Label] != blank or
                        variantMetafield[tableContent_dimension2] != blank or 
                        variantMetafield[tableContent_dimension3Label] != blank or
                        variantMetafield[tableContent_dimension3] != blank or
                        variantMetafield[tableContent_dimension4Label] != blank or
                        variantMetafield[tableContent_dimension4] != blank or
                        variantMetafield[tableContent_rhodium_platedLabel] != blank or 
                        variantMetafield[tableContent_rhodium_plated] != blank or 
                        variantMetafield[tableContent_frc_ttl_crt_wgt] != blank or
                        variantMetafield[tableContent_dec_ttl_crt_wgt] != blank or
                        variantMetafield[prodtableContent_dimension1Label] != blank or
                        variantMetafield[prodtableContent_dimension1] != blank or
                        variantMetafield[prodtableContent_dimension2Label] != blank or
                        variantMetafield[prodtableContent_dimension2] != blank or
                        variantMetafield[prodtableContent_dimension3Label] != blank or
                        variantMetafield[prodtableContent_dimension3] != blank or
                        variantMetafield[prodtableContent_dimension4Label] != blank or
                        variantMetafield[prodtableContent_dimension4] != blank 
                    %}    
                    <div class="product-info-item">
                        <div class="product-info-holder">
                            {% if productMetafield[prodtableContent_title] != blank %}<h5>{{ productMetafield[prodtableContent_title] }}</h5>{% endif %}

                            <div class="product-info-table">
                                <figure class="table">
                                    <table>
                                        <tbody>
                                            {%- if hide_default_title -%}
                                            {%- else -%}
                                                <tr data-tooltip="jewgoldktclr" data-row="pdp-metal" data-cont="abcd" > 
                                                    <td><span class="pdp-tab-label">{% if productMetafield[prodtableContent_metalLabel] != blank %}{{ productMetafield[prodtableContent_metalLabel] }}{% else %}Metal{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val"><span class="variantColorTitle" data-variant-title>{{ product.selected_or_first_available_variant.option1  }}</span></span></td>
                                                </tr>
                                            {% endif %}

                                            {% if variantMetafield[tableContent_dimension1] != blank %}
                                                <tr data-tooltip="jewdim1" data-row="{{ tableContent_dimension1 }}">
                                                    <td><span class="pdp-tab-label">{% if  variantMetafield[tableContent_dimension1Label] != blank %}{{  variantMetafield[tableContent_dimension1Label]}} {% else %}Height{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_dimension1] }}</span>{%- if productMetafield[prodtableContent_tooltip_height] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[prodtableContent_tooltip_height] }}"></span>{%- endif -%}</td>
                                                </tr>
                                                {% endif %}
                                                
                                                {% if variantMetafield[tableContent_dimension2] != blank %}
                                                <tr data-tooltip="jewdim2" data-row="{{ tableContent_dimension2 }}">
                                                    <td><span class="pdp-tab-label">{% if  variantMetafield[tableContent_dimension2Label] != blank %}{{  variantMetafield[tableContent_dimension2Label]}} {% else %}Width{% endif %}</span></td>
