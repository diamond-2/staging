                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_rhodium_plated] | truncate: 30, '...' }}</span> {% if productMetafield[tableContent_tooltip_rhodium] != blank %} <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_rhodium]}}"></span>{% endif %}</td>
                                                </tr>
                                                {% endif %}


                                                {% comment %}
                                                {% if variantMetafield[tableContent_frc_ttl_crt_wgt] != blank %}
                                                <tr data-tooltip="jewrhodium" data-row="tableContent_common_ttl_crt_wgt">
                                                    <td><span class="pdp-tab-label" data-metafield="tableContent_frc_ttl_crt_wgt">Carat Total weight(CT. TW.)</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_frc_ttl_crt_wgt] | truncate: 30, '...' }}</span> {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt]}}"></span>{% endif %}</td>
                                                </tr>
                                                {% endif %}
                                                 {% endcomment %}

                                                {% if variantMetafield[tableContent_dec_ttl_crt_wgt] != blank  or variantMetafield[tableContent_frc_ttl_crt_wgt] != blank %} 
                                                <tr data-tooltip="jewrhodium" data-row="{% if variantMetafield[tableContent_dec_ttl_crt_wgt] != blank %}{{ tableContent_dec_ttl_crt_wgt }}{% else %}{{ tableContent_frc_ttl_crt_wgt }}{% endif %}">
                                                    <td><span class="pdp-tab-label">{% if variantMetafield[tableContent_dec_ttl_crt_wgt] != blank %}Total carat weight(cts.){% else %}Carat Total weight(CT. TW.){% endif %}</span></td>
                                                    <td>
                                                    <span class="pdp-tab-val test-2">{% if variantMetafield[tableContent_dec_ttl_crt_wgt] != blank %}{{ variantMetafield[tableContent_dec_ttl_crt_wgt] | times: 100 | round:2 | money_without_currency  }}{% else %}{{ variantMetafield[tableContent_frc_ttl_crt_wgt] | truncate: 30, '...' }}{% endif %}</span> 
                                                    {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} 
                                                    <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt] }}"></span>
                                                    {% endif %}
                                                    
                                                    </td>
                                                </tr>
                                                {% endif %}
                                                
                                                {% if variantMetafield[prodtableContent_dimension1] != blank %} 
                                                <tr data-tooltip="jewrhodium" data-row="{{ prodtableContent_dimension1 }}">
                                                    <td><span class="pdp-tab-label">{{ variantMetafield[prodtableContent_dimension1Label] }}</span></td>
                                                    <td>
                                                    <span class="pdp-tab-val test-2">{{ variantMetafield[prodtableContent_dimension1]}}</span> 
                                                    {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} 
                                                    <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt] }}"></span>
                                                    {% endif %}
                                                    
                                                    </td>
                                                </tr>
                                                {% endif %}                                               

                                                {% if variantMetafield[prodtableContent_dimension2] != blank %} 
                                                <tr data-tooltip="jewrhodium" data-row="{{ prodtableContent_dimension2 }}">
                                                    <td><span class="pdp-tab-label">{{ variantMetafield[prodtableContent_dimension2Label] }}</span></td>
                                                    <td>
                                                    <span class="pdp-tab-val test-2">{{ variantMetafield[prodtableContent_dimension2]}}</span> 
                                                    {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} 
                                                    <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt] }}"></span>
                                                    {% endif %}
                                                    
                                                    </td>
                                                </tr>
                                                {% endif %}

                                                {% if variantMetafield[prodtableContent_dimension3] != blank %} 
                                                <tr data-tooltip="jewrhodium" data-row="{{ prodtableContent_dimension3Label }}">
                                                    <td><span class="pdp-tab-label">{{ variantMetafield[prodtableContent_dimension3Label] }}</span></td>
                                                    <td>
                                                    <span class="pdp-tab-val test-2">{{ variantMetafield[prodtableContent_dimension3]}}</span> 
                                                    {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} 
                                                    <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt] }}"></span>
                                                    {% endif %}
                                                    
                                                    </td>
                                                </tr>
                                                {% endif %}

                                                {% if variantMetafield[prodtableContent_dimension4] != blank %} 
                                                <tr data-tooltip="jewrhodium" data-row="{{ prodtableContent_dimension4 }}">
                                                    <td><span class="pdp-tab-label">{{ variantMetafield[prodtableContent_dimension4Label] }}</span></td>
                                                    <td>
                                                    <span class="pdp-tab-val test-2">{{ variantMetafield[prodtableContent_dimension4]}}</span> 
                                                    {% if productMetafield[tableContent_tooltip_ttl_crt_wgt] != blank %} 
                                                    <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_ttl_crt_wgt] }}"></span>
                                                    {% endif %}
                                                    
                                                    </td>
                                                </tr>
                                                {% endif %}
                                                
                                        </tbody>
                                    </table>
                                </figure>
                            </div>
                        </div>
                    </div>
                    {% endif %}
                    
                    {%- capture tableContent_rstab_title-%}rstab_size1_title{%- endcapture -%}
                    {%- capture tableContent_rstab1-%}rstab_size1_row1{%- endcapture -%}
                    {%- capture tableContent_rstab_avg_color-%}rstab_size1_avg_color{%- endcapture -%}
                    {%- capture tableContent_rstab_avg_clarity-%}rstab_size1_avg_clarity{%- endcapture -%}
