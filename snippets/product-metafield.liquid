                    variantMetafield[tableContent_dimension1] != blank or 
                    variantMetafield[tableContent_dimension2Label] != blank or
                    variantMetafield[tableContent_dimension2] != blank or
                    variantMetafield[tableContent_dimension3Label] != blank or
                    variantMetafield[tableContent_dimension3] != blank or 
                    variantMetafield[tableContent_dimension4Label] != blank or
                    variantMetafield[tableContent_dimension4] != blank or 
                    variantMetafield[tableContent_rhodium_platedLabel] != blank or
                    variantMetafield[tableContent_rhodium_plated] != blank or
                    variantMetafield[tableContent_frc_ttl_crt_wgt] != blank or 
                    variantMetafield[tableContent_dec_ttl_crt_wgt] != blank 
