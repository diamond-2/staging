                    {% if
                        productMetafield[prodtableContent_rstab_title] != blank or 
                        productMetafield[prodtableContent_rstab1] != blank or 
                        productMetafield[prodtableContent_rstab_avg_colorLabel] != blank or 
                        productMetafield[prodtableContent_rstab_avg_color] != blank or 
                        productMetafield[prodtableContent_rstab_avg_clarityLabel] != blank or 
                        productMetafield[prodtableContent_rstab_avg_clarity] != blank or
                        productMetafield[prodtableContent_rstab_setting_typeLabel] != blank or  
                        productMetafield[prodtableContent_rstab_setting_type] != blank 
