                    <div class="product-info-item">
                        <div class="product-info-holder">
                            {% if productMetafield[prodtableContent_rstab_title] != blank %}<h5>{{ productMetafield[prodtableContent_rstab_title] }}</h5>{% endif %}

                            <div class="product-info-table">
                                <figure class="table">
                                    <table>
                                        <tbody>
                                            {% for i in (1..30) %}
                                            {%- capture tableContent_rstab-%}rstab_size1_row{{i}}{%- endcapture -%}

                                            {% if productMetafield[prodtableContent_rstab] != blank %}
                                                {% assign rsData =  productMetafield[prodtableContent_rstab] | split: '||' %}
                                                <tr data-row="{{ productMetafield[prodtableContent_rstab] }}">
                                                    <td>{{ rsData[0] }}</td>
                                                    <td>{{ rsData[1] }}</td>
                                                    <td>{{ rsData[2] }}</td>
                                                </tr>
                                            {% endif %}
                                            {% endfor %}
                                            {% if productMetafield[prodtableContent_rstab_avg_color] != blank %}
                                                <tr data-tooltip="jewstncoloura" data-row="{{ productMetafield[prodtableContent_rstab_avg_color] }}">
                                                    <td><span class="pdp-tab-label">{% if productMetafield[prodtableContent_rstab_avg_colorLabel] != blank %}{{ productMetafield[prodtableContent_rstab_avg_colorLabel] }}{% else %}Average Color{% endif %}</span></td>
                                                    <td> </td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[prodtableContent_rstab_avg_color] }}</span></td>
                                                </tr>
                                            {% endif %}
                                            {% if productMetafield[prodtableContent_rstab_avg_clarity] != blank %}
                                                <tr data-tooltip="jewstnclaritya" data-row="{{ productMetafield[prodtableContent_rstab_avg_clarity] }}">
                                                    <td><span class="pdp-tab-label">{% if productMetafield[prodtableContent_rstab_avg_clarityLabel] != blank %}{{ productMetafield[prodtableContent_rstab_avg_clarityLabel] }}{% else %}Average Clarity{% endif %}</span></td>
                                                    <td> </td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[prodtableContent_rstab_avg_clarity] }}</span></td>
                                                </tr>
                                            {% endif %}
                                            {% if productMetafield[prodtableContent_rstab_setting_type] != blank %}
                                                <tr data-tooltip="jewstnsetting" data-row="{{ productMetafield[prodtableContent_rstab_setting_type] }}">
                                                    <td><span class="pdp-tab-label">{% if productMetafield[prodtableContent_rstab_setting_typeLabel] != blank %}{{ productMetafield[prodtableContent_rstab_setting_typeLabel] }}{% else %}Setting Type{% endif %}</span></td>
                                                    <td> </td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[prodtableContent_rstab_setting_type] }}</span></td>
                                                </tr>
                                            {% endif %}
                                        </tbody>
                                    </table>
                                </figure>
                            </div>
                        </div>
                    </div>
                    {% endif %}


                </div>
                {% endif %}   
                {% endif %}

                <div class="product-info-gird_column">
