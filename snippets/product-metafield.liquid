<!-- Product Metafield Code -->

<div class="product-details-wrapper pdp-tabber show_tab" id="productDetails">
    <div class="mobile-pdp-acc-trigger show_mobile"><p>PRODUCT DETAILS</p></div>
    <div class="pdp-acc-body">
        <h3><span class="pdp-acc-body-heading">{{ product.title }} {% if hide_default_title %} {% else %}in  <span class="variantColorTitle" data-variant-title>{{ product.selected_or_first_available_variant.option1  }}</span>{% endif %}</span>{% if template.suffix contains 'loose-stone'%}<a class="product-view-report" href="javascript:;" aria-label="">View report&gt;</a>{% endif %}</h3>    
        <div class="product-details-row">
            <div class="product-info-gird">
                {%- capture tableContent_title -%}prod_info_title{%- endcapture -%}
                {%- capture tableContent_dimension1Label  -%}prod_info_dimension1_label{% endcapture %}
                {%- capture tableContent_dimension1  -%}prod_info_dimension1{% endcapture %}
                {%- capture tableContent_dimension2Label  -%}prod_info_dimension2_label{% endcapture %}
                {%- capture tableContent_dimension2  -%}prod_info_dimension2{% endcapture %}
                {%- capture tableContent_rhodium_plated -%}rhodium_plated{% endcapture %}
                {%- capture tableContent_avg_clarity -%}prod_info_avg_clarity{%- endcapture -%}
                {%- capture tableContent_stone_category -%}prod_info_stone_category{%- endcapture -%}
                {%- capture tableContent_stone_shape -%}prod_info_stone_shape{%- endcapture -%}
                {%- capture tableContent_stone_cut_shape -%}prod_info_stone_cut_shape{%- endcapture -%}
                {%- capture tableContent_no_of_stones-%}prod_info_no_of_stones{%- endcapture -%}


                
                {% if template.suffix != 'loose-stone' %}
                {% if
                    productMetafield[tableContent_title] != blank or 
                    variantMetafield[tableContent_dimension1Label] != blank or
                    variantMetafield[tableContent_dimension1] != blank or 
                    variantMetafield[tableContent_dimension1Label] != blank or
                    variantMetafield[tableContent_dimension2] != blank or 
                    variantMetafield[tableContent_rhodium_plated] != blank 
                %}       
                <div class="product-info-gird_column">  
                    
                    {% if
                        productMetafield[tableContent_title] != blank or 
                        variantMetafield[tableContent_dimension1Label] != blank or
                        variantMetafield[tableContent_dimension1] != blank or 
                        variantMetafield[tableContent_dimension1Label] != blank or
                        variantMetafield[tableContent_dimension2] != blank or 
                        variantMetafield[tableContent_rhodium_plated] != blank 
                    %}    
                    <div class="product-info-item">
                        <div class="product-info-holder">
                            {% if productMetafield[tableContent_title] != blank %}<h5>{{ productMetafield[tableContent_title] }}</h5>{% endif %}

                            <div class="product-info-table">
                                <figure class="table">
                                    <table>
                                        <tbody>
                                            {%- if hide_default_title -%}
                                            {%- else -%}
                                                <tr data-tooltip="jewgoldktclr" data-row="pdp-metal" > 
                                                    <td><span class="pdp-tab-label">Metal</span></td>
                                                    <td><span class="pdp-tab-val"><span class="variantColorTitle" data-variant-title>{{ product.selected_or_first_available_variant.option1  }}</span></span></td>
                                                </tr>
                                            {% endif %}

                                            {% if variantMetafield[tableContent_dimension1] != blank %}
                                                <tr data-tooltip="jewdim1" data-row="{{ tableContent_dimension1 }}">
                                                    <td><span class="pdp-tab-label">{% if  variantMetafield[tableContent_dimension1Label] != blank %}{{  variantMetafield[tableContent_dimension1Label]}} {% else %}Height{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_dimension1] }}</span>{%- if productMetafield[tableContent_tooltip_height] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_height] }}"></span>{%- endif -%}</td>
                                                </tr>
                                                {% endif %}
                                                
                                                {% if variantMetafield[tableContent_dimension2] != blank %}
                                                <tr data-tooltip="jewdim2" data-row="{{ tableContent_dimension2 }}">
                                                    <td><span class="pdp-tab-label">{% if  variantMetafield[tableContent_dimension2Label] != blank %}{{  variantMetafield[tableContent_dimension2Label]}} {% else %}Width{% endif %}</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_dimension2] }}</span> {%- if productMetafield[tableContent_tooltip_width] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_width] }}"></span>{%- endif -%}</td>
                                                </tr>
                                                {% endif %}
                                                {% if variantMetafield[tableContent_rhodium_plated] != blank %}
                                                <tr data-tooltip="jewrhodium" data-row="{{ tableContent_rhodium_plated }}">
                                                    <td><span class="pdp-tab-label">Rhodium plated</span></td>
                                                    <td><span class="pdp-tab-val">{{ variantMetafield[tableContent_rhodium_plated] | truncate: 30, '...' }}</span> {% if productMetafield[tableContent_tooltip_rhodium] != blank %} <span class="meta_tooltip" data-tooltip-title="{{productMetafield[tableContent_tooltip_rhodium]}}"></span>{% endif %}</td>
                                                </tr>
                                                {% endif %}
                                        </tbody>
                                    </table>
                                </figure>
                            </div>
                        </div>
                    </div>
                    {% endif %}

                    
                    {%- capture tableContent_rstab_title-%}rstab_size1_title{%- endcapture -%}
                    {%- capture tableContent_rstab1-%}rstab_size1_row1{%- endcapture -%}
                    {%- capture tableContent_rstab_avg_color-%}rstab_size1_avg_color{%- endcapture -%}
                    {%- capture tableContent_rstab_avg_clarity-%}rstab_size1_avg_clarity{%- endcapture -%}
                    {%- capture tableContent_rstab_setting_type-%}rstab_size1_setting_type{%- endcapture -%}
                    
                    {% if
                        productMetafield[tableContent_rstab_title] != blank or 
                        productMetafield[tableContent_rstab1] != blank or 
                        productMetafield[tableContent_rstab_avg_color] != blank or 
                        productMetafield[tableContent_rstab_avg_clarity] != blank or 
                        productMetafield[tableContent_rstab_setting_type] != blank 
                    %}    
                    <div class="product-info-item">
                        <div class="product-info-holder">
                            {% if productMetafield[tableContent_rstab_title] != blank %}<h5>{{ productMetafield[tableContent_rstab_title] }}</h5>{% endif %}

                            <div class="product-info-table">
                                <figure class="table">
                                    <table>
                                        <tbody>
                                            {% for i in (1..30) %}
                                            {%- capture tableContent_rstab-%}rstab_size1_row{{i}}{%- endcapture -%}

                                            {% if productMetafield[tableContent_rstab] != blank %}
                                                {% assign rsData =  productMetafield[tableContent_rstab] | split: '||' %}
                                                <tr data-row="{{ productMetafield[tableContent_rstab] }}">
                                                    <td>{{ rsData[0] }}</td>
                                                    <td>{{ rsData[1] }}</td>
                                                    <td>{{ rsData[2] }}</td>
                                                </tr>
                                            {% endif %}
                                            {% endfor %}
                                            {% if productMetafield[tableContent_rstab_avg_color] != blank %}
                                                <tr data-tooltip="jewstncoloura" data-row="{{ productMetafield[tableContent_rstab_avg_color] }}">
                                                    <td><span class="pdp-tab-label">Average Color</span></td>
                                                    <td> </td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[tableContent_rstab_avg_color] }}</span></td>
                                                </tr>
                                            {% endif %}
                                            {% if productMetafield[tableContent_rstab_avg_clarity] != blank %}
                                                <tr data-tooltip="jewstnclaritya" data-row="{{ productMetafield[tableContent_rstab_avg_clarity] }}">
                                                    <td><span class="pdp-tab-label">Average Clarity</span></td>
                                                    <td> </td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[tableContent_rstab_avg_clarity] }}</span></td>
                                                </tr>
                                            {% endif %}
                                            {% if productMetafield[tableContent_rstab_setting_type] != blank %}
                                                <tr data-tooltip="jewstnsetting" data-row="{{ productMetafield[tableContent_rstab_setting_type] }}">
                                                    <td><span class="pdp-tab-label">Setting Type</span></td>
                                                    <td> </td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[tableContent_rstab_setting_type] }}</span></td>
                                                </tr>
                                            {% endif %}
                                        </tbody>
                                    </table>
                                </figure>
                            </div>
                        </div>
                    </div>
                    {% endif %}


                </div>
                {% endif %}   
                {% endif %}

                <div class="product-info-gird_column">
                {% for j in (1..10) %}
                {%- capture otherTableContent_title -%}dia_tab{{ j }}_title{%- endcapture -%}
                {%- capture tableContent_avg_clarity -%}dia_tab{{ j }}_avg_clarity{%- endcapture -%}
                {%- capture tableContent_stone_category -%}dia_tab{{ j }}_stone_category{%- endcapture -%}
                {%- capture tableContent_stone_shape -%}dia_tab{{ j }}_stone_shape{%- endcapture -%}
                {%- capture tableContent_stone_cut_shape -%}dia_tab{{ j }}_stone_cut_shape{%- endcapture -%}                
                {%- capture tableContent_stone_name -%}dia_tab{{ j }}stone_name{%- endcapture -%}
                {%- capture tableContent_no_of_stones-%}dia_tab{{ j }}_no_of_stones{%- endcapture -%}
                {%- capture tableContent_min_stn_ttl_crt_wt-%}dia_tab{{ j }}_min_stn_ttl_crt_wt{%- endcapture -%}
                {%- capture tableContent_stone_avg_color -%}dia_tab{{ j }}_stone_avg_color{%- endcapture -%}
                {%- capture tableContent_stone_type -%}dia_tab{{ j }}_stone_type{%- endcapture -%}
                {%- capture tableContent_cert_cut -%}dia_tab{{ j }}_cert_cut{%- endcapture -%}
                {%- capture tableContent_stone_coating -%}dia_tab{{ j }}_stone_coating{%- endcapture -%}
                {%- capture tableContent_color_grading -%}dia_tab{{ j }}_color_grading{%- endcapture -%}
                {%- capture tableContent_clarity_grading -%}dia_tab{{ j }}_clarity_grading{%- endcapture -%}
                {%- capture tableContent_cert_carats -%}dia_tab{{ j }}_cert_carats{%- endcapture -%}
                {%- capture tableContent_cert_fluorescence -%}dia_tab{{ j }}_cert_fluorescence{%- endcapture -%}
                {%- capture tableContent_cert_depth -%}dia_tab{{ j }}_cert_depth{%- endcapture -%}
                {%- capture tableContent_cert_table -%}dia_tab{{ j }}_cert_table{%- endcapture -%}
                {%- capture tableContent_cert_polish -%}dia_tab{{ j }}_cert_polish{%- endcapture -%}
                {%- capture tableContent_cert_symmentry -%}dia_tab{{ j }}_cert_symmentry{%- endcapture -%}
                {%- capture tableContent_cert_girdle -%}dia_tab{{ j }}_cert_girdle{%- endcapture -%}
                {%- capture tableContent_cert_culet -%}dia_tab{{ j }}_cert_culet{%- endcapture -%}
                {%- capture tableContent_cert_measurements -%}dia_tab{{ j }}_cert_measurements{%- endcapture -%}
                {%- capture tableContent_actual_stone_wt_carat -%}dia_tab{{ j }}_actual_stone_wt_carat{%- endcapture -%}
                

                {% if
                    productMetafield[otherTableContent_title] != blank or 
                    productMetafield[tableContent_stone_category ] != blank or
                    productMetafield[tableContent_stone_shape] != blank or 
                    productMetafield[tableContent_stone_cut_shape] != blank or 
                    productMetafield[tableContent_no_of_stones] != blank or 
                    productMetafield[tableContent_min_stn_ttl_crt_wt] != blank or
                    productMetafield[tableContent_stone_avg_color] != blank or
                    productMetafield[tableContent_stone_type] != blank or
                    productMetafield[tableContent_cert_cut] != blank or
                    productMetafield[tableContent_stone_coating] != blank or
                    productMetafield[tableContent_clarity_grading] != blank or
                    productMetafield[tableContent_color_grading] != blank or
                    productMetafield[tableContent_cert_carats] != blank or
                    productMetafield[tableContent_cert_fluorescence] != blank or
                    productMetafield[tableContent_cert_depth] != blank or
                    productMetafield[tableContent_cert_table] != blank or
                    productMetafield[tableContent_cert_polish] != blank or
                    productMetafield[tableContent_cert_symmentry] != blank or
                    productMetafield[tableContent_cert_girdle] != blank or
                    productMetafield[tableContent_cert_culet] != blank or
                    productMetafield[tableContent_stone_name] != blank or 
                    
                    productMetafield[tableContent_actual_stone_wt_carat] != blank or
                    productMetafield[tableContent_cert_measurements] != blank
                %}
                
                    <div class="product-info-item">
                        <div class="product-info-holder">
                            {% if productMetafield[otherTableContent_title] != blank %}<h5>{{ productMetafield[otherTableContent_title] }}</h5>{% endif %}
                            <div class="product-info-table">
                                <figure class="table">
                                    <table>
                                        <tbody>
                                            {% if template.suffix contains 'loose-stone' %}
                                            {% if productMetafield[tableContent_stone_cut_shape] != blank %}
                                                <tr data-tooltip="stncertshape" data-row="{{ productMetafield[tableContent_stone_cut_shape] | truncate: 15 | handleize }}">
                                                    <td><span class="pdp-tab-label">Shape</span></td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[tableContent_stone_cut_shape] | truncate: 15 }}</span></td>
                                                </tr>
                                                {% endif %}                                            
                                            {% else %}
                                                {% if productMetafield[tableContent_stone_shape] != blank or productMetafield[tableContent_stone_category ] != blank %}
                                                <tr data-tooltip="jewstncnt" data-row="{{ productMetafield[tableContent_stone_shape] | truncate: 30 | prepend: 'Number of ' | handleize }}{{ productMetafield[tableContent_stone_category ] | truncate: 30 | append: 'Gemstones' | handleize }}">
                                                    <td><span class="pdp-tab-label">{{ productMetafield[tableContent_stone_shape] | truncate: 30 | prepend: 'Number of ' }} {{ productMetafield[tableContent_stone_category ] | truncate: 30 | append: 'Gemstones' }}</span></td>
                                                    <td><span class="pdp-tab-val">{{ productMetafield[tableContent_no_of_stones] }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip">{{ productMetafield[tableContent_tooltip_rhodium] }}</span></td>
                                                </tr>
                                                {% endif %}
                                            {% endif %}
                                            {% if productMetafield[tableContent_min_stn_ttl_crt_wt] != blank %}
                                            <tr data-tooltip="jewstntcw" data-row="{{ tableContent_min_stn_ttl_crt_wt }}">
                                                <td><span class="pdp-tab-label">Minimum carat total weight (CT. TW.)</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_min_stn_ttl_crt_wt] }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}
                                            {% if productMetafield[tableContent_stone_avg_color] != blank %}
                                            <tr data-tooltip="jewstncolourm" data-row="{{ tableContent_stone_avg_color }}">
                                                <td><span class="pdp-tab-label">Average Color</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_stone_avg_color] | truncate: 30 }}</span> {%- if productMetafield[tableContent_tooltip_avg_color] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_avg_color] }}"></span>{%- endif -%}</td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_stone_name] != blank %}
                                            <tr data-tooltip="jewstnname" data-row="{{ tableContent_stone_name }}">
                                                <td><span class="pdp-tab-label">Stone Name</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_stone_name] }}</span> {%- if productMetafield[tableContent_tooltip_avg_name] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_avg_name] }}"></span>{%- endif -%}</td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_actual_stone_wt_carat] != blank %}
                                            <tr data-tooltip="jewstnname" data-row="{{ tableContent_actual_stone_wt_carat }}">
                                                <td><span class="pdp-tab-label">Actual stone Wtc. </span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_actual_stone_wt_carat] }}</span> {%- if productMetafield[tableContent_tooltip_actual_stone_wt_carat] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_actual_stone_wt_carat] }}"></span>{%- endif -%}</td>
                                            </tr>
                                            {% endif %}                                           

                                            {% if productMetafield[tableContent_avg_clarity] != blank %}
                                            <tr data-tooltip="jewstnclaritym" data-row="{{ tableContent_avg_clarity }}">
                                                <td><span class="pdp-tab-label">Average Clarity</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_avg_clarity] }}</span> {%- if productMetafield[tableContent_tooltip_avg_clarity] != blank -%}<span class="meta_tooltip" data-tooltip-title="{{ productMetafield[tableContent_tooltip_avg_clarity] }}"></span>{%- endif -%}</td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_stone_type] != blank %}
                                            <tr data-tooltip="jewstnsetting" data-row="{{ tableContent_stone_type }}">
                                                <td><span class="pdp-tab-label">Setting type</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_stone_type] | truncate: 22 | append: ' setting' }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_cut] != blank %}
                                            <tr data-tooltip="stncertcut" data-row="{{ tableContent_cert_cut }}">
                                                <td><span class="pdp-tab-label">Cut</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_cut] | truncate: 15 }}</span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_color_grading] != blank %}
                                            <tr data-tooltip="stncertclr" data-row="{{ tableContent_color_grading }}">
                                                <td><span class="pdp-tab-label">Color</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_color_grading] | truncate: 30 }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}


                                            {% if productMetafield[tableContent_clarity_grading] != blank %}
                                            <tr data-tooltip="stncertclarity" data-row="{{ tableContent_clarity_grading }}">
                                                <td><span class="pdp-tab-label">Clarity</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_clarity_grading] | truncate: 15 }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_carats] != blank %}
                                            <tr data-tooltip="stncertwt" data-row="{{ tableContent_cert_carats }}">
                                                <td><span class="pdp-tab-label">Carat Weight</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_carats] }}</span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_fluorescence] != blank %}
                                            <tr data-tooltip="stncertflr" data-row="{{ tableContent_cert_fluorescence }}">
                                                <td><span class="pdp-tab-label">Fluorescence</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_fluorescence] }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_depth] != blank %}
                                            <tr data-tooltip="stncertdepth" data-row="{{ tableContent_cert_depth }}">
                                                <td><span class="pdp-tab-label">Depth %</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_depth] }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_table] != blank %}
                                            <tr data-tooltip="stncerttable" data-row="{{ tableContent_cert_table }}">
                                                <td><span class="pdp-tab-label">Table %</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_table] | truncate: 15 }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_polish] != blank %}
                                            <tr data-tooltip="stncertpolish" data-row="{{ tableContent_cert_polish }}">
                                                <td><span class="pdp-tab-label">Polish</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_polish] | truncate: 15 }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_symmentry] != blank %}
                                            <tr data-tooltip="stncertsymmetry" data-row="{{ tableContent_cert_symmentry }}">
                                                <td><span class="pdp-tab-label">Symmetry</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_symmentry] | truncate: 15 }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_girdle] != blank %}
                                            <tr data-tooltip="stncertgirdle" data-row="{{ tableContent_cert_girdle }}">
                                                <td><span class="pdp-tab-label">Girdle</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_girdle] | truncate: 15 }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_culet] != blank %}
                                            <tr data-tooltip="stncertculet" data-row="{{ tableContent_cert_culet }}">
                                                <td><span class="pdp-tab-label">Culet</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_culet] | truncate: 15 }}</span> <span class="meta_tooltip" data-tooltip-title="tooltip"></span></td>
                                            </tr>
                                            {% endif %}

                                            {% if productMetafield[tableContent_cert_measurements] != blank %}
                                            <tr data-tooltip="stncertmesure" data-row="{{ tableContent_cert_measurements }}">
                                                <td><span class="pdp-tab-label">Measurements</span></td>
                                                <td><span class="pdp-tab-val">{{ productMetafield[tableContent_cert_measurements] | truncate: 30 }}</span></td>
                                            </tr>
                                            {% endif %}

                                            {% if template.suffix contains 'loose-stone' and  current_variant.sku != blank %}
                                            <tr data-tooltip="stncertserial" data-row="sku">
                                                <td><span class="pdp-tab-label">Stock Number</span></td>
                                                <td><span class="pdp-tab-val">{{ current_variant.sku }}</span></td>
                                            </tr>
                                            {% endif %}
                                        </tbody>
                                    </table>
                                </figure>
                            </div>
                        </div>
                    </div>                

                {% endif %}
                {% endfor %}
            </div>
            {% if template.suffix == 'loose-stone' %}
            <div class="product-info-gird_column stone__info">
                <ul>
                    <li>
                        <h5>Diamond2 Measurement</h5>
                        <div class="stone__info_img"><img data-src="{{ 'ls_measurement.png' | file_url }}" class="lazyload" alt=""/></div>
                    </li>
                    <li>
                        <h5>Diamond2 Top View</h5>
                        <div class="stone__info_img"><img data-src="{{ 'ls_topview.png' | file_url }}" class="lazyload" alt=""/></div>
                        <div class="stone__info_img"><img data-src="{{ 'ls_bottomview.png' | file_url }}" class="lazyload" alt=""/></div>
                    </li>
                </ul>

            </div>
            {% endif %}
        </div>
        </div>
    </div>
</div>